
<a href="<%= room_url(room) %>" data-room-id="<%= room.id %>">
  <div class="room-container group">

    <figure class="room-image-default">
      <% unless room.pictures.empty? %>
        <img src="<%= room.pictures.sample.image.url(:profile) %>">
      <% else %>
        <img src="<%= Picture.new.image.url(:profile) %>">
      <% end %>
    </figure>

    <figure class="owner-photo">
      <img src="<%= room.owner.avatar.url(:thumb) %>">
    </figure>

    <article class="room-info-small">
      <%= MAKE_NICE[room.room_type] %><br>
      <%= "#{ room.display_title }" %>
    </article>


  </div>
</a>